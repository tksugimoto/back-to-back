{
  "manifest_version": 2,

  "name": "Back To Back",
  "short_name": "Back To Back",
  "version": "0.0.4",
  "description": "Restore the backspace keyboard shortcut",
  "author": "Andrew C",

  "icons": {
    "16": "ico/16.png",
    "48": "ico/48.png",
    "128": "ico/128.png"
  },

  "content_scripts": [{
    "run_at": "document_start",
    "matches": ["<all_urls>"],
    "js": ["listener.js"]
  }],

  "minimum_chrome_version": "52.0.2720.0"

}
